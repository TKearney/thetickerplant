block content
  html
    //body(onload='myFunction()', style='margin:0;')
    body
      .cover#cover
      .loader#loader
      .myDiv.animate-bottom(style='display:none;')
      .container-fluid
        a(href="archi")
         button(type="button") Architecture
        form.form-inline#form1
          .form-group
            label(for='number') Lookback hours:
            input#email.form-control(type='number', step='0.01')
          button.btn.btn-default(type='submit') Submit
        //svg#light(width='100%',height=300)
        //svg#temp(width='100%',height=300)
        object#sensorToggle.emb(data='images/sensorToggle.svg', type='image/svg+xml', width='100%')
          | Your browser doesn't support SVG
        #chart(style='width:100%; height:300px; background-color: #D0E3C4;')
        .row
          .column
            img(src='images/test.jpg', class='img-responsive', alt='thetickerplant')
          .column
            video(controls='')
              source(src='images/output.mp4', type='video/mp4')
              | Your browser does not support the video tag.
      script(type='text/javascript', src='javascripts/load.js')
      script(type='text/javascript', src='javascripts/chart.js')
      script(type='text/javascript', src='javascripts/websocket.js')
